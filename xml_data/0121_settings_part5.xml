<profile>
   <id>snapshot</id>
   <repositories>
      <repository>
        <snapshots>
          <enabled>true</enabled>
        </snapshots>
        <id>maven-demo-snapshots</id>
        <name>maven-demo-snapshots</name>
        <url>http://${env.NEXUSSERVERIP}:8081/repository/maven-demo-snapshots/</url>
      </repository>
    </repositories>
    <activation>
      <activeByDefault>true</activeByDefault>
    </activation>
</profile>